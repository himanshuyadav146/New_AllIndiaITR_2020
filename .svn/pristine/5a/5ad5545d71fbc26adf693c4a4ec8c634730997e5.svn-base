<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".submitDocument.ITROrderStatusActivity">

    <androidx.appcompat.widget.Toolbar
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:background="@color/colorPrimary"
            app:theme="@style/ToolbarColoredBackArrow"
            android:id="@+id/toolbar">

        <TextView
                android:id="@+id/toolbar_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_sixteen"
                android:layout_gravity="center"
                android:gravity="center_horizontal"
                android:textAlignment="center"
                android:singleLine="true"
                android:layout_centerHorizontal="true"
                style="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"/>

    </androidx.appcompat.widget.Toolbar>

    <FrameLayout
            android:id="@+id/relativelayout_frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/toolbar">

        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/eighty"
                android:background="@color/colorPrimary"/>

        <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="115dp"
                android:layout_marginTop="@dimen/fifteen"
                android:layout_marginLeft="@dimen/fifteen"
                android:layout_marginRight="@dimen/fifteen"
                android:layout_marginBottom="@dimen/ten"
                app:cardCornerRadius="@dimen/fifteen"
                app:cardElevation="@dimen/two"
                android:translationZ="1dp">

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <LinearLayout
                        android:layout_toLeftOf="@+id/linearlayout_info"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginTop="@dimen/ten"
                        android:layout_marginLeft="@dimen/thirty"
                        android:layout_marginRight="@dimen/ten"
                        android:layout_marginBottom="@dimen/ten"
                        android:layout_alignParentLeft="true"
                        android:gravity="center_vertical">

                    <TextView
                            android:id="@+id/username_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            android:textSize="@dimen/text_size_twenty"
                            android:textStyle="bold"
                            android:textColor="@color/light_black"/>

                    <TextView
                            android:id="@+id/pan_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/two"
                            android:textSize="@dimen/text_size_twelve"
                            android:textColor="@color/dark_grey"/>

                    <TextView
                            android:id="@+id/dob_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/two"
                            android:textSize="@dimen/text_size_twelve"
                            android:textColor="@color/dark_grey"/>

                    <TextView
                            android:id="@+id/mobile_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/two"
                            android:textSize="@dimen/text_size_twelve"
                            android:textColor="@color/dark_grey"/>

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/linearlayout_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginBottom="@dimen/fifteen"
                        android:layout_marginRight="@dimen/fifteen"
                        android:gravity="center_vertical">

                    <ImageView
                            android:id="@+id/img_itr_status"
                            android:layout_width="16dp"
                            android:layout_height="16dp" />

                    <TextView
                            android:id="@+id/tv_status"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="4dp"
                            android:textSize="@dimen/text_size_twelve"
                            android:textColor="@color/red"/>

                </LinearLayout>

            </RelativeLayout>

        </androidx.cardview.widget.CardView>

    </FrameLayout>

    <RelativeLayout
            android:id="@+id/rl_inprogress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativelayout_frame"
            android:layout_above="@+id/rl_chat"
            android:layout_marginTop="1dp">

        <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/fifteen"
                    android:layout_marginTop="25dp"
                    android:layout_marginBottom="40dp"
                    android:orientation="vertical">

                <LinearLayout
                        android:id="@+id/ll_payment_success"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                    <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                        <ImageView
                                android:id="@+id/img_payment_success"
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:background="@drawable/circle"
                                android:padding="@dimen/two"
                                android:gravity="center"/>

                        <View
                                android:id="@+id/view_payment_success"
                                android:layout_width="@dimen/two"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal" />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/ten">

                        <TextView
                                android:id="@+id/txt_payment_success"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/ten"
                                android:layout_marginTop="-4dp"
                                android:layout_gravity="center_vertical"
                                android:text="Payment Received"
                                android:textColor="@color/lighter_black"
                                android:textSize="@dimen/text_size_fourteen" />

                        <net.cachapa.expandablelayout.ExpandableLayout
                                android:id="@+id/expandlayout_payment_success"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/exp_ll_payment_success"
                                    android:layout_marginBottom="@dimen/twelve"
                                    app:cardBackgroundColor="@color/lightest_grey"
                                    app:cardElevation="0dp">

                                <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                    <TextView
                                            android:id="@+id/tv_payment_desc"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="@dimen/fifteen"
                                            android:layout_marginTop="@dimen/twelve"
                                            android:layout_marginBottom="@dimen/twelve"
                                            android:layout_marginRight="@dimen/fifteen"
                                            android:padding="@dimen/two"
                                            android:textStyle="normal"
                                            android:textColor="@color/lighter_black"
                                            android:textSize="@dimen/text_size_fourteen"
                                            android:lineSpacingExtra="@dimen/two" />

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </net.cachapa.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/ll_expert_assign"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                    <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                        <ImageView
                                android:id="@+id/img_expert_assign"
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:background="@drawable/circle"
                                android:padding="@dimen/two"
                                android:gravity="center"/>

                        <View
                                android:id="@+id/view_expert_assign"
                                android:layout_width="@dimen/two"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal" />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/ten">

                        <TextView
                                android:id="@+id/txt_expert_assign"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/ten"
                                android:layout_marginTop="-4dp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/expert_assign"
                                android:textColor="@color/lighter_black"
                                android:textSize="@dimen/text_size_fourteen" />

                        <net.cachapa.expandablelayout.ExpandableLayout
                                android:id="@+id/expandlayout_expert_assign"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                    android:id="@+id/exp_ll_expert_assign"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/twelve"
                                    app:cardBackgroundColor="@color/lightest_grey"
                                    app:cardElevation="0dp">

                                <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                    <TextView
                                            android:id="@+id/tv_taxexpert"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="@dimen/fifteen"
                                            android:layout_marginTop="@dimen/twelve"
                                            android:layout_marginBottom="@dimen/twelve"
                                            android:layout_marginRight="@dimen/fifteen"
                                            android:padding="@dimen/two"
                                            android:textStyle="normal"
                                            android:textColor="@color/lighter_black"
                                            android:textSize="@dimen/text_size_fourteen"
                                            android:lineSpacingExtra="@dimen/two" />

                                    <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="23dp"
                                            android:layout_marginTop="@dimen/two"
                                            android:layout_marginRight="23dp"
                                            android:layout_marginBottom="25dp"
                                            android:padding="@dimen/two"
                                            android:text="Your Tax Expert will contact you as early as possible during working hours (10 AM - 7PM)."
                                            android:textColor="@color/lighter_black"
                                            android:textSize="@dimen/text_size_fourteen"
                                            android:visibility="gone"/>

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </net.cachapa.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/ll_review_info"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                    <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                        <ImageView
                                android:id="@+id/img_review_info"
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:background="@drawable/circle"
                                android:padding="@dimen/two"
                                android:gravity="center"/>

                        <View
                                android:id="@+id/view_review_info"
                                android:layout_width="@dimen/two"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal" />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/ten">

                        <TextView
                                android:id="@+id/txt_review_info"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/ten"
                                android:layout_marginTop="-4dp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/review_info"
                                android:textColor="@color/lighter_black"
                                android:textSize="@dimen/text_size_fourteen" />

                        <net.cachapa.expandablelayout.ExpandableLayout
                                android:id="@+id/expandlayout_review_info"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                    android:id="@+id/exp_ll_review_info"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/twelve"
                                    app:cardBackgroundColor="@color/lightest_grey"
                                    app:cardElevation="0dp">

                                <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                    <TextView
                                            android:id="@+id/tv_review_info"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textSize="@dimen/text_size_fourteen"
                                            android:textColor="@color/lighter_black"
                                            android:layout_marginTop="@dimen/twelve"
                                            android:layout_marginLeft="@dimen/fifteen"
                                            android:layout_marginRight="@dimen/fifteen"
                                            android:layout_marginBottom="@dimen/twelve"
                                            android:padding="@dimen/two"/>

                                    <LinearLayout
                                            android:id="@+id/ll_show"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical"
                                            android:layout_marginLeft="@dimen/fifteen"
                                            android:layout_marginRight="@dimen/fifteen"
                                            android:layout_marginBottom="@dimen/twelve"
                                            android:visibility="gone">


                                    </LinearLayout>

                                    <Button
                                            android:id="@+id/btn_review_info"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="@string/click_update"
                                            android:textAllCaps="false"
                                            android:textColor="@color/white"
                                            android:backgroundTint="@color/red"
                                            android:layout_gravity="center_horizontal"
                                            android:paddingLeft="30dp"
                                            android:paddingRight="30dp"
                                            android:layout_marginBottom="@dimen/ten"
                                            android:visibility="gone"/>

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </net.cachapa.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                        android:id="@+id/ll_e_filed"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/ten">

                    <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                        <ImageView
                                android:id="@+id/img_e_filed"
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:background="@drawable/circle"
                                android:padding="@dimen/two"
                                android:gravity="center"/>

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="@dimen/ten">

                        <TextView
                                android:id="@+id/txt_e_filed"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/ten"
                                android:layout_marginTop="-4dp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/e_filed"
                                android:textColor="@color/lighter_black"
                                android:textSize="@dimen/text_size_fourteen" />

                        <net.cachapa.expandablelayout.ExpandableLayout
                                android:id="@+id/expandlayout_e_filed"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/ten">

                            <androidx.cardview.widget.CardView
                                    android:id="@+id/exp_ll_e_filed"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/twelve"
                                    app:cardBackgroundColor="@color/lightest_grey"
                                    app:cardElevation="0dp">

                                <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                    <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_horizontal"
                                            android:gravity="left"
                                            android:textSize="@dimen/text_size_fourteen"
                                            android:textColor="@color/lighter_black"
                                            android:layout_marginTop="5dp"
                                            android:layout_marginBottom="@dimen/fifteen"
                                            android:layout_marginLeft="@dimen/fifteen"
                                            android:layout_marginRight="@dimen/fifteen"
                                            android:padding="@dimen/two"
                                            android:lineSpacingExtra="@dimen/two"/>

                                </LinearLayout>

                            </androidx.cardview.widget.CardView>

                        </net.cachapa.expandablelayout.ExpandableLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </ScrollView>

    </RelativeLayout>

    <RelativeLayout
            android:id="@+id/rl_completed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativelayout_frame"
            android:layout_marginTop="25dp"
        android:visibility="gone">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginBottom="@dimen/ten"
                android:layout_marginRight="@dimen/fifteen"
                android:layout_marginLeft="@dimen/fifteen">

            <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    app:srcCompat="@drawable/ic_confirmation"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/twelve"
                    android:layout_marginBottom="@dimen/two"/>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/orderstatus_efield_success"
                    android:textStyle="bold"
                    android:textSize="@dimen/text_size_sixteen"
                    android:textColor="@color/light_green"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="5dp"
                    android:padding="@dimen/two"/>

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="@string/orderstatus_efield_ack"
                    android:textSize="@dimen/text_size_fourteen"
                    android:textColor="@color/lighter_black"
                    android:layout_marginTop="5dp"
                    android:gravity="center"/>

            <androidx.cardview.widget.CardView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/three"
                    app:cardBackgroundColor="@color/lightest_grey"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/ten"
                    app:cardElevation="0dp">

                <TextView
                        android:id="@+id/tv_acknowledgementno"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="@dimen/ten"
                        android:paddingBottom="@dimen/ten"
                        android:paddingRight="@dimen/twenty"
                        android:paddingLeft="@dimen/twenty"
                        android:textStyle="bold"
                        android:textSize="@dimen/text_size_fourteen"
                        android:textColor="@color/lighter_black" />

            </androidx.cardview.widget.CardView>

        </LinearLayout>


    </RelativeLayout>

    <TextView
            android:id="@+id/note"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:layout_gravity="center_horizontal"
            android:gravity="center_horizontal"
            android:text="@string/orderstatus_efield_notice"
            android:textSize="@dimen/text_size_twelve"
            android:textColor="@color/lighter_black"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="@dimen/ten"
            android:layout_marginLeft="@dimen/fifteen"
            android:layout_marginRight="@dimen/fifteen"
            android:layout_above="@+id/btn_e_verify"
            android:visibility="gone"/>

    <Button
            android:id="@+id/btn_e_verify"
            android:layout_width="match_parent"
            android:layout_height="@dimen/forty"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="@dimen/fifteen"
            android:layout_marginLeft="@dimen/fifteen"
            android:layout_marginBottom="7dp"
            android:textAllCaps="false"
            android:text="@string/e_verify"
            android:textColor="@color/white"
            android:background="@drawable/button_background"
            android:textSize="@dimen/text_size_fourteen"
            android:visibility="gone"/>

    <RelativeLayout
            android:id="@+id/rl_chat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/forty"
                android:orientation="horizontal"
                android:weightSum="3"
                android:layout_marginTop="@dimen/fifteen"
                android:layout_marginLeft="@dimen/fifteen"
                android:layout_marginRight="@dimen/fifteen"
                android:layout_marginBottom="@dimen/ten">

            <androidx.cardview.widget.CardView
                    android:id="@+id/btn_call"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:cardCornerRadius="@dimen/three"
                    app:cardBackgroundColor="@color/light_grey"
                    android:layout_marginEnd="@dimen/two"
                    app:cardElevation="0dp">

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp">

                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/ic_ringer_volume_100px"
                            android:layout_gravity="center"/>

                    <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/orderstatus_call"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_size_fourteen"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textStyle="normal"
                            android:fontFamily="sans-serif-medium"/>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                    android:id="@+id/btn_chat"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:cardCornerRadius="@dimen/three"
                    app:cardBackgroundColor="@color/blue"
                    android:layout_marginEnd="@dimen/two"
                    android:layout_marginStart="@dimen/two"
                    app:cardElevation="0dp">

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp">

                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/ic_communication_100px_2"
                            android:layout_gravity="center"/>

                    <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/orderstatus_chat"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_size_fourteen"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textStyle="normal"
                            android:fontFamily="sans-serif-medium"/>

                </LinearLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                    android:id="@+id/btn_email"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:cardCornerRadius="@dimen/three"
                    app:cardBackgroundColor="@color/light_grey"
                    android:layout_marginStart="@dimen/two"
                    app:cardElevation="0dp">

                <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:paddingLeft="25dp"
                        android:paddingRight="25dp">

                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/ic_secured_letter_100px"
                            android:layout_gravity="center"/>

                    <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
