            android:layout_marginEnd="@dimen/fifteen"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/skip_button" android:layout_marginStart="@dimen/fifteen"
            android:layout_marginTop="@dimen/fifteen" android:layout_marginBottom="8dp">

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="85dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:weightSum="7.6" android:id="@+id/linearLayout3">

            <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.65"
                    android:background="@color/white"/>

            <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:srcCompat="@drawable/ic_tutorial_upload_document"
                    android:id="@+id/imageView24"
                    android:layout_weight="1.3"/>

            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.2"
                    android:weightSum="17">

                <View
                        android:layout_width="1dp"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="@color/dark_grey"
                        android:layout_gravity="center"/>

                <View
                        android:layout_width="1dp"
                        android:layout_height="0dp"
                        android:layout_weight="1"/>

                <View
